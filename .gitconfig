[core]
	autocrlf = input
[color]
	ui = true
[commit]
	gpgsign = true
[gpg]
	program = gpg
[color "diff"]
	meta = yellow bold
	old = red bold
	new = green bold
[color "status"]
	added = green bold
	changed = yellow
	untracked = cyan
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
